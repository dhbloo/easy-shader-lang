struct Meter <T> {
    length : T;
    func Meter(self, l : T) {
        self.length = l
    }
}

struct CentiMeter <T> {
    length : T;
    func CentiMeter (self, l : T) {
        self.length = l
    }
}

convert<T>(m : Meter<T>) -> CentiMeter<T>  {
    return CentiMeter<T>(m.length * 100);
}

