struct Meter <T> {
    length : T;
    func Meter(self, l : T) {
        self.length = l;
    }
};

struct CentiMeter <B> {
    length : B;
    func CentiMeter (self, l : B) {
        self.length = l;
    }
};

convert<T>(m : Meter<T>) -> CentiMeter<T>  {
    return CentiMeter<T>(m.length * 100);
}

convert<T>(m : CentiMeter<T>) -> Meter<T>  {
    return Meter<T>(m.length * 100);
}


convert (a : CentiMeter) -> Meter;